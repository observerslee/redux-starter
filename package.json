{
  "name": "redux-starter",
  "version": "0.0.1",
  "title": "React Redux Starter",
  "description": "React Redux Starter With Webpack",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "test": "jest",
    "lint": "eslint src",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.production.js --progress --hide-modules -p"
  },
  "author": "Observerslee",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "cross-env": "^3.2.3",
    "css-loader": "^0.27.1",
    "directory-named-webpack-plugin": "^2.1.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.17.1",
    "eslint-config-standard-jsx": "^3.3.0",
    "eslint-config-standard-react": "^4.3.0",
    "eslint-plugin-react": "^6.10.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.2",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-css-modules": "^4.1.0",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "\\.(jpeg|jpg|png|gif|svg)$": "<rootDir>/__mocks__/noopMock.js"
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "standard-react"
    ],
    "env": {
      "node": true,
      "browser": true
    },
    "rules": {
      "jsx-quotes": [
        "warn",
        "prefer-double"
      ]
    }
  }
}
